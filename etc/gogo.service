[Unit]
Description=Go app server
After=redis-server.service

[Service]
Type=simple
User=root
Group=root
WorkingDirectory=/var/www/gogo/cur

ExecStart=/var/www/gogo/cur/gogo --addr 0.0.0.0:443 --cert /etc/letsencrypt/live/go.waits.io/fullchain.pem --key /etc/letsencrypt/live/go.waits.io/privkey.pem

[Install]
WantedBy=multi-user.target
